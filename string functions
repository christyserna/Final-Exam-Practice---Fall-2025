#include <stdio.h>
#include <string.h>

int main() {

char s1[] = "Apple";
char s2[] = "Applet";

int res = strcmp(s1, s2);

if (res == 0)
  printf("s1 and s2 are the same");
else if (res == < 0)
  printf("s1 is smaller than s1");
else
  printf("s1 is greater than s2");

return 0;
}



// USE THE STRING.H HEADER FILE, AND THE STRCMP



#include <stdio.h>

void printStr(char str[]) {
  printf("%s", str);
  }

  int main() {
    char str[] = "CITE30103";

    //passing string to a function
    printStr(str);
    return 0;
  }
