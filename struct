// practicing struct

#include <stdio.h>
#include <string.h>

struct Person {
char name[50];
int citNo;
float salary;
} person1; 

int main() {
strcpy(person1.name, "Christy Serna");
person1.citNo = 2004;
person1.salary = 5000;

printf("Name: %s\n", person1.name);
printf("CitNo: %d\n", person1.citNo);
printf("salary: %d\n", person1.salary);

return 0;
}

-------------------------------------

#include <string.h>

struct Person {
char name[50];
int citNo;
float salary;
};

int main() {
struct Person person1, person2, p[20];
return 0;
}


OR


struct Person{
char name[50];
int citNo;
float salary;
} person1, person2, p[20];


